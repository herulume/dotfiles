Config { font = "xft:Fira Code:size=10:bold:antialias=true"
    , bgColor = "black"
    , fgColor = "grey"
    , position = Top
    , lowerOnStart = True
    , commands = [ Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
             , Run Memory ["-t","Mem: <usedratio>%"] 10
             , Run Swap [ ] 10
             , Run Date "%a %b %_d %l:%M" "date" 10
             , Run Battery ["Bat0"] 600
             ]
    , sepChar = "%"
    , alignSep = "}{"
    , template = "%cpu%  %memory%  %swap%  %battery% }{ <fc=#49E20E>%date%</fc>"
    }
